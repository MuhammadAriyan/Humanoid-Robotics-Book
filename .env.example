# Environment Configuration for Ary's Physical Humanoid Robotics
# Copy this file to .env and fill in your actual values

# Database Configuration
NEON_DATABASE_URL=postgresql://your-neon-db-url-here

# AI Service Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENAI_BASE_URL=https://openrouter.ai/api/v1

# Application Environment
APP_ENV=production

# CORS Configuration (for production)
BACKEND_CORS_ORIGINS=["https://muhammadariyan.github.io","https://ary-s-physical-humanoid-robotics.com"]

